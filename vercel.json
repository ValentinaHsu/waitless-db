{
    "version": 2,
    "builds": [
        {
            "src": "api/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/menu",
            "dest": "/api/menu.js"
        },
        {
            "src": "/menu/:id",
            "dest": "/api/menu-item.js"
        }
    ]
}